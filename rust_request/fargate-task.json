{
    "family": "sonar-fargate",
    "networkMode": "awsvpc",
    "taskRoleArn": "arn:aws:iam::748887400437:role/ecsTaskArn",
    "executionRoleArn": "arn:aws:iam::748887400437:role/service-role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sonar_request",
            "image": "daiterdg/release_sonar_rust:latest",
            "essential": true,
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "/aws/ecs/default/release_sonar_rust-80d0-45c3",
                "awslogs-region": "eu-north-1",
                "awslogs-stream-prefix": "ecs",
                "awslogs-create-group": "true"
              }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
